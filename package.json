{
  "name": "playpost",
  "version": "1.0.11",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "device": "ENVFILE=.env.local react-native run-ios --device",
    "test:watch": "jest --watch --silent --verbose",
    "lint": "tslint -c tslint.json 'src/**/*' --project tsconfig.json",
    "lint:fix": "tslint -c tslint.json 'src/**/*' --fix --project tsconfig.json",
    "lint:debug": "tslint -c tslint.json 'src/**/*' --debug --project tsconfig.json",
    "typescript": "tsc",
    "upgrade-interactive": "npm-check --update",
    "tsc": "tsc -p ./ --noEmit",
    "tsc:watch": "tsc -p ./ --noEmit -w",
    "postversion": "react-native-version"
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts/"
    ]
  },
  "engines": {
    "node": "^10.15.0",
    "npm": "^6.8.0"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "@react-native-community/async-storage": "^1.5.0",
    "@react-native-community/netinfo": "^3.2.1",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-native": "^0.57.65",
    "@types/react-native-restart": "0.0.0",
    "@types/react-native-vector-icons": "^6.4.1",
    "@types/react-native-video": "^3.1.4",
    "@types/react-navigation": "^3.0.7",
    "@types/react-redux": "^7.1.1",
    "@types/react-test-renderer": "^16.8.2",
    "@types/url-parse": "^1.4.3",
    "@types/valid-url": "^1.0.2",
    "appcenter": "^2.1.0",
    "appcenter-analytics": "^2.1.0",
    "appcenter-crashes": "^2.1.0",
    "axios": "^0.19.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-plugin-functional-hmr": "^1.0.25",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "core-js": "^3.1.4",
    "date-fns": "^1.30.1",
    "detox": "^13.0.1",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-native": "^3.7.0",
    "fsevents": "^2.0.7",
    "jest": "^24.8.0",
    "jest-fetch-mock": "^2.1.2",
    "jest-runner": "^24.8.0",
    "jest-runtime": "^24.8.0",
    "metro-react-native-babel-preset": "^0.54.1",
    "npm-check": "^5.9.0",
    "react": "^16.8.6",
    "react-fast-compare": "^2.0.4",
    "react-native": "0.59.9",
    "react-native-app-intro-slider": "^2.0.1",
    "react-native-config": "^0.11.7",
    "react-native-deep-linking": "^2.2.0",
    "react-native-draggable-flatlist": "^1.1.7",
    "react-native-elements": "^1.1.0",
    "react-native-fs": "^2.13.3",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-iap": "^3.0.0",
    "react-native-keychain": "^3.1.3",
    "react-native-reanimated": "^1.1.0",
    "react-native-settings-screen": "^2.1.2",
    "react-native-share-extension": "git+https://github.com/alinz/react-native-share-extension.git#3db5320135c154c367d72c44688a29ce41b210e7",
    "react-native-splash-screen": "^3.2.0",
    "react-native-swift": "^1.2.2",
    "react-native-testing-library": "^1.9.1",
    "react-native-track-player": "^1.1.4",
    "react-native-typescript-transformer": "^1.2.12",
    "react-native-vector-icons": "^6.6.0",
    "react-native-version": "^3.1.0",
    "react-native-version-number": "^0.3.6",
    "react-native-video": "^4.4.2",
    "react-native-webview": "5.8.2",
    "react-navigation": "^3.11.0",
    "react-navigation-animated-switch": "^0.2.0",
    "react-redux": "^7.1.0",
    "react-test-renderer": "^16.8.6",
    "redux": "^4.0.1",
    "redux-axios-middleware": "^4.0.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^5.10.0",
    "redux-saga": "^1.0.4",
    "reselect": "^4.0.0",
    "rn-fetch-blob": "^0.10.15",
    "ts-jest": "^24.0.2",
    "tslib": "^1.10.0",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.2",
    "typescript-tslint-plugin": "^0.5.3",
    "url-parse": "^1.4.7",
    "valid-url": "^1.0.9",
    "why-did-you-update": "^1.0.6"
  },
  "jest": {
    "preset": "react-native",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx,json,node}",
      "!**/node_modules/**",
      "!**/typings/**",
      "!**/integration/**",
      "!**/e2e/**"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.(ts|tsx|js)",
      "**/?(*.)+(spec|test).(ts|tsx|js)"
    ],
    "forceCoverageMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|tsx|js)",
      "<rootDir>/src/**/?(*.)+(spec|test).(ts|tsx|js)"
    ],
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/tests/e2e"
    ],
    "cacheDirectory": ".jest/cache",
    "setupFiles": [
      "<rootDir>/tests/setup.ts",
      "<rootDir>/node_modules/react-native-gesture-handler/jestSetup.js"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/Playpost.app",
        "build": "xcodebuild -workspace ios/Playpost.xcworkspace -scheme Playpost -configuration Debug -destination 'platform=iOS Simulator,name=iPhone 7' -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone X"
      }
    },
    "test-runner": "jest"
  },
  "isSwift": true
}
