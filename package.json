{
  "name": "playpost",
  "version": "1.3.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "concurrently \"npm run tsc\" \"npm run lint\" \"npm run jest\"",
    "test:typescript": "npm run tsc",
    "test:lint": "npm run lint",
    "test:jest": "npm run jest",
    "jest": "jest --silent",
    "jest:coverage": "jest --silent --coverage",
    "device:ios": "ENVFILE=.env.local react-native run-ios --device",
    "ios:ipad": "ENVFILE=.env.local react-native run-ios --simulator 'iPad Pro (12.9-inch) (3rd generation) (13.0)'",
    "device:android": "ENVFILE=.env.local react-native run-android",
    "device:android:release": "ENVFILE=.env.local react-native run-android --variant release",
    "device:release": "ENVFILE=.env.local react-native run-ios --device --configuration Release",
    "test:watch": "jest --watch --silent --verbose --coverage",
    "lint": "tslint -c tslint.json 'src/**/*.ts{,x}'",
    "lint:fix": "tslint -c tslint.json 'src/**/*.ts{,x}' --fix",
    "lint:debug": "tslint -c tslint.json 'src/**/*' --debug --project tsconfig.json",
    "typescript": "tsc",
    "upgrade-interactive": "npm-check --update",
    "tsc": "tsc -p ./ --noEmit",
    "tsc:watch": "tsc -p ./ --noEmit -w",
    "postinstall": "npx jetify",
    "version": "react-native-version",
    "release": "auto-changelog -p && git add CHANGELOG.md && git commit -m 'Update changelog' && git push && git push --tags --no-verify"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "engines": {
    "node": "^10.15.0",
    "npm": "^6.8.0"
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "keepachangelog",
    "unreleased": true,
    "commitLimit": false
  },
  "dependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/async-storage": "^1.6.2",
    "@react-native-community/netinfo": "^4.2.2",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^24.0.18",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^12.7.8",
    "@types/react": "^16.9.3",
    "@types/react-native": "^0.60.16",
    "@types/react-native-restart": "0.0.0",
    "@types/react-native-vector-icons": "^6.4.2",
    "@types/react-native-video": "^3.1.5",
    "@types/react-navigation": "^3.0.8",
    "@types/react-redux": "^7.1.4",
    "@types/react-test-renderer": "^16.9.0",
    "@types/url-parse": "^1.4.3",
    "@types/valid-url": "^1.0.2",
    "appcenter": "^2.3.0",
    "appcenter-analytics": "^2.3.0",
    "appcenter-crashes": "^2.3.0",
    "axios": "^0.19.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-const-enum": "0.0.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "core-js": "^3.2.1",
    "currency-symbol-map": "^4.0.4",
    "date-fns": "^2.3.0",
    "detox": "^14.3.4",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^2.1.2",
    "jest-runner": "^24.9.0",
    "jest-runtime": "^24.9.0",
    "lodash.debounce": "^4.0.8",
    "metro-react-native-babel-preset": "^0.56.0",
    "npm-check": "^5.9.0",
    "react": "16.9.0",
    "react-fast-compare": "^2.0.4",
    "react-native": "0.61.1",
    "react-native-app-intro-slider": "^3.0.0",
    "react-native-config": "git+https://github.com/luggit/react-native-config#89a602bf8be3808838403a97afaf915caeec76c2",
    "react-native-deep-linking": "^2.2.0",
    "react-native-device-info": "^3.1.4",
    "react-native-draggable-flatlist": "^1.1.9",
    "react-native-elements": "^1.2.3",
    "react-native-fs": "^2.14.1",
    "react-native-gesture-handler": "^1.4.1",
    "react-native-iap": "^3.4.13",
    "react-native-inappbrowser-reborn": "^3.1.0",
    "react-native-keychain": "^4.0.0",
    "react-native-localize": "^1.2.1",
    "react-native-modal": "^11.4.0",
    "react-native-reanimated": "^1.3.0",
    "react-native-screens": "^1.0.0-alpha.23",
    "react-native-share-extension": "git+https://github.com/jvandenaardweg/react-native-share-extension#3a7be58a624fc1f975494bb8f80992146916b464",
    "react-native-splash-screen": "^3.2.0",
    "react-native-swift": "^1.2.2",
    "react-native-testing-library": "^1.11.1",
    "react-native-track-player": "^2.0.0-rc13",
    "react-native-typescript-transformer": "^1.2.13",
    "react-native-vector-icons": "^6.6.0",
    "react-native-version": "^3.2.0",
    "react-native-video": "^5.0.2",
    "react-native-webview": "^7.2.7",
    "react-navigation": "^3.12.1",
    "react-navigation-animated-switch": "^0.3.2",
    "react-redux": "^7.1.1",
    "react-test-renderer": "^16.9.0",
    "redux": "^4.0.4",
    "redux-axios-middleware": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.1",
    "reselect": "^4.0.0",
    "rn-fetch-blob": "^0.10.16",
    "ts-jest": "^24.1.0",
    "tslib": "^1.10.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3",
    "typescript-tslint-plugin": "^0.5.4",
    "url-parse": "^1.4.7",
    "valid-url": "^1.0.9",
    "why-did-you-update": "^1.0.6"
  },
  "jest": {
    "preset": "react-native",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx,json,node}",
      "!**/node_modules/**",
      "!**/typings/**",
      "!**/integration/**",
      "!**/e2e/**"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.(ts|tsx|js)",
      "**/?(*.)+(spec|test).(ts|tsx|js)"
    ],
    "forceCoverageMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|tsx|js)",
      "<rootDir>/src/**/?(*.)+(spec|test).(ts|tsx|js)"
    ],
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/tests/e2e"
    ],
    "cacheDirectory": ".jest/cache",
    "setupFiles": [
      "<rootDir>/tests/setup.ts",
      "<rootDir>/node_modules/react-native-gesture-handler/jestSetup.js"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/Playpost.app",
        "build": "xcodebuild -workspace ios/Playpost.xcworkspace -scheme Playpost -configuration Debug -destination 'platform=iOS Simulator,name=iPhone 7' -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone X"
      }
    },
    "test-runner": "jest"
  },
  "isSwift": true,
  "devDependencies": {
    "auto-changelog": "^1.16.1",
    "concurrently": "^4.1.2",
    "deep-object-diff": "^1.1.0",
    "husky": "^3.0.5",
    "jetifier": "^1.6.4",
    "tslint-config-prettier": "^1.18.0"
  }
}
