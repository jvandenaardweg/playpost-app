{
  "name": "playpost",
  "version": "1.2.0-0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "concurrently \"npm run tsc\" \"npm run lint\" \"npm run jest\"",
    "test:typescript": "npm run tsc",
    "test:lint": "npm run lint",
    "test:jest": "npm run jest",
    "jest": "jest",
    "jest:coverage": "jest --coverage",
    "device": "ENVFILE=.env.local react-native run-ios --device",
    "device:release": "ENVFILE=.env.local react-native run-ios --device --configuration Release",
    "test:watch": "jest --watch --silent --verbose --coverage",
    "lint": "tslint -c tslint.json 'src/**/*.ts{,x}'",
    "lint:fix": "tslint -c tslint.json 'src/**/*.ts{,x}' --fix",
    "lint:debug": "tslint -c tslint.json 'src/**/*' --debug --project tsconfig.json",
    "typescript": "tsc",
    "upgrade-interactive": "npm-check --update",
    "tsc": "tsc -p ./ --noEmit",
    "tsc:watch": "tsc -p ./ --noEmit -w",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "postversion": "react-native-version && git push && git push --tags --no-verify"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts/"
    ]
  },
  "engines": {
    "node": "^10.15.0",
    "npm": "^6.8.0"
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "keepachangelog",
    "unreleased": true,
    "commitLimit": false
  },
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@react-native-community/async-storage": "^1.6.1",
    "@react-native-community/netinfo": "^3.2.1",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^24.0.17",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^12.7.2",
    "@types/react": "^16.8.23",
    "@types/react-native": "^0.57.65",
    "@types/react-native-restart": "0.0.0",
    "@types/react-native-vector-icons": "^6.4.1",
    "@types/react-native-video": "^3.1.4",
    "@types/react-navigation": "^3.0.8",
    "@types/react-redux": "^7.1.2",
    "@types/react-test-renderer": "^16.8.3",
    "@types/url-parse": "^1.4.3",
    "@types/valid-url": "^1.0.2",
    "appcenter": "^2.2.0",
    "appcenter-analytics": "^2.2.0",
    "appcenter-crashes": "^2.2.0",
    "axios": "^0.19.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-functional-hmr": "^1.0.25",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "core-js": "^3.2.1",
    "currency-symbol-map": "^4.0.4",
    "date-fns": "^1.30.1",
    "detox": "^14.0.2",
    "fsevents": "^2.0.7",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^2.1.2",
    "jest-runner": "^24.9.0",
    "jest-runtime": "^24.9.0",
    "lodash.debounce": "^4.0.8",
    "metro-react-native-babel-preset": "^0.54.1",
    "npm-check": "^5.9.0",
    "react": "^16.8.3",
    "react-fast-compare": "^2.0.4",
    "react-native": "0.59.9",
    "react-native-app-intro-slider": "^3.0.0",
    "react-native-config": "^0.11.7",
    "react-native-deep-linking": "^2.2.0",
    "react-native-device-info": "^2.3.2",
    "react-native-draggable-flatlist": "^1.1.7",
    "react-native-elements": "^1.1.0",
    "react-native-fs": "^2.13.3",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-iap": "^3.3.9",
    "react-native-keychain": "^3.1.3",
    "react-native-reanimated": "^1.2.0",
    "react-native-screens": "^1.0.0-alpha.22",
    "react-native-share-extension": "git+https://github.com/jvandenaardweg/react-native-share-extension.git#8bf3d82b821591dcac06fa38a8b215f25b11cd92",
    "react-native-splash-screen": "^3.2.0",
    "react-native-swift": "^1.2.2",
    "react-native-testing-library": "^1.11.1",
    "react-native-track-player": "^1.1.6",
    "react-native-typescript-transformer": "^1.2.13",
    "react-native-vector-icons": "^6.6.0",
    "react-native-version": "^3.1.0",
    "react-native-video": "^4.4.4",
    "react-native-webview": "5.8.0",
    "react-navigation": "^3.11.1",
    "react-navigation-animated-switch": "^0.2.1",
    "react-redux": "^7.1.0",
    "react-test-renderer": "^16.8.6",
    "redux": "^4.0.4",
    "redux-axios-middleware": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^5.10.0",
    "redux-saga": "^1.0.5",
    "reselect": "^4.0.0",
    "rn-fetch-blob": "0.10.15",
    "ts-jest": "^24.0.2",
    "tslib": "^1.10.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3",
    "typescript-tslint-plugin": "^0.5.4",
    "url-parse": "^1.4.7",
    "valid-url": "^1.0.9",
    "why-did-you-update": "^1.0.6"
  },
  "jest": {
    "preset": "react-native",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx,json,node}",
      "!**/node_modules/**",
      "!**/typings/**",
      "!**/integration/**",
      "!**/e2e/**"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.(ts|tsx|js)",
      "**/?(*.)+(spec|test).(ts|tsx|js)"
    ],
    "forceCoverageMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|tsx|js)",
      "<rootDir>/src/**/?(*.)+(spec|test).(ts|tsx|js)"
    ],
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/tests/e2e"
    ],
    "cacheDirectory": ".jest/cache",
    "setupFiles": [
      "<rootDir>/tests/setup.ts",
      "<rootDir>/node_modules/react-native-gesture-handler/jestSetup.js"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/Playpost.app",
        "build": "xcodebuild -workspace ios/Playpost.xcworkspace -scheme Playpost -configuration Debug -destination 'platform=iOS Simulator,name=iPhone 7' -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone X"
      }
    },
    "test-runner": "jest"
  },
  "isSwift": true,
  "devDependencies": {
    "auto-changelog": "^1.15.0",
    "concurrently": "^4.1.2",
    "deep-object-diff": "^1.1.0",
    "husky": "^3.0.4",
    "tslint-config-prettier": "^1.18.0"
  }
}
